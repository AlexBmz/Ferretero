@using SistemaWEB.Ferretero.Models
@model TipoDocumentoBEAN
@{
    ViewBag.Title = "EditarTipoDoc";
}

<h2>Editar Tipo Documento</h2>

@using (Html.BeginForm())
{
    <div class="form-horizontal">
        <div>
            @Html.HiddenFor(model => model.IdTipoDoc, htmlAttributes: new { @class = "form-control" })
        </div>
       
        <div class="form-group">
            @Html.LabelFor(model => model.Descripcion, htmlAttributes: new { @class = "control-label" })
            @Html.TextBoxFor(model => model.Descripcion, htmlAttributes: new { @class = "form-control" })
        </div>      
        <div class="form-group">
            <input type="submit" value="Actualizar" class="btn btn-success" />
        </div>

    </div>
}
